{
  "name": "my-vue-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:network": "vite --host",
    "build": "node scripts/optimize-images.cjs && tsc -b && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:network": "vite preview --host",
    "serve": "npm run build && npm run preview:network",
    "network-info": "node scripts/get-network-info.js",
    "optimize-images": "node scripts/optimize-images.cjs",
    "lighthouse": "npx lighthouse http://localhost:5173 --output=html --output-path=./lighthouse-report.html",
    "build:lib": "tsc lib/*.ts --outDir lib"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.17.1",
    "@mui/lab": "^5.0.0-alpha.136",
    "@mui/material": "^5.15.6",
    "@types/react-slick": "^0.23.13",
    "axios": "^1.10.0",
    "dotenv": "^17.0.1",
    "framer-motion": "^12.23.0",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-player": "^2.16.0",
    "react-qr-code": "^2.0.17",
    "react-router-dom": "^6.21.3",
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1",
    "swiper": "^11.2.10",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/pg": "^8.15.4",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vercel/node": "^5.3.3",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^16.2.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^5.0.12",
    "vite-plugin-pwa": "^1.0.0"
  }
}
